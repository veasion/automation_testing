{
  "COMMAND_INTERVAL": 125,
  "TIMEOUT_PAGE_LOAD": 20,
  "CHROME_DRIVER_PATH": "auto",
  "FIREFOX_DRIVER_PATH": null,
  "DRIVER_OPTIONS": {
    "arguments": [
      "no-sandbox",
      "--disable-popup-blocking",
      "--disable-blink-features=AutomationControlled",
      "--ignore-certificate-errors"
    ],
    "experimentalOptions": {
      "excludeSwitches": [
        "enable-automation"
      ]
    },
    "proxy": {},
    "capability": {
      "loggingPrefs": {
        "performance": "ALL"
      }
    }
  },
  "OCR": {
    "baidu": {
      "appId": null,
      "apiKey": null,
      "secretKey": null,
      "params": {
        "paragraph": false,
        "detect_direction": true,
        "recognize_granularity": "small"
      }
    },
    "default": "baidu"
  }
}